        <script>
            var webpackManifestBuild = (function() {
                var json = null;
                $.ajax({
                    'async': false,
                    'global': false,
                    'url': "./scripts/chunk-manifest-build.json",
                    'dataType': "json",
                    'success': function (data) {
                        json = data;
                    }
                });
                return json;
            })();
            var link = "./scripts/" + webpackManifestBuild["0"];
            var element = document.createElement('script');
            element.src = link;
            window.onload = function() {
                document.getElementsByTagName('body')[0].appendChild(element);
            }
            var basePath = "//<%= host %>";
        </script>
